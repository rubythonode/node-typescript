{
  "name": "node-typescript",
  "version": "0.1.0",
  "description": "The Project clone from MEAN.JS and modify code with TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/thanhtruong0315/node-typescript.git"
  },
  "author": "Thanh Truong",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "gulp build && tsc",
    "serve": "cd dist && nodemon --trace-warnings server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "export NODE_ENV=test && npm run build && cd dist && nyc mocha --trace-warnings -t 10000 **/test/server/*.tests.js",
    "posttest": "export CODECOV_TOKEN=c913eb73-2dd5-4d06-8d5f-259febbf9cb2 && nyc report --reporter=json && codecov -f coverage/*.json",
    "test:debug": "NODE_ENV=test && npm run build && cd dist && mocha --debug-brk **/test/server/*.tests.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "acl": "^0.4.11",
    "async": "^2.1.2",
    "aws-sdk": "^2.138.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "chai": "^4.1.2",
    "compression": "^1.6.2",
    "connect-mongo": "~2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-hbs": "^1.0.4",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.3",
    "fbgraph": "^1.3.0",
    "fs": "0.0.1-security",
    "generate-password": "^1.3.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "lodash": "^4.17.4",
    "lusca": "^1.4.1",
    "mongoose": "^4.11.13",
    "morgan": "^1.7.0",
    "nodemailer": "^4.2.0",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pug": "^2.0.0-beta6",
    "request": "^2.78.0",
    "semver": "^5.4.1",
    "validator": "^9.0.0",
    "winston": "^2.4.0"
  },
